{
  "name": "t3brightside/gallerycontent",
  "description": "TYPO3 CMS extension to create image gallery as a content element.",
  "type": "typo3-cms-extension",
  "version": "0.1.5",
  "keywords": ["TYPO3", "extension", "images", "gallery", "content"],
  "homepage": "https://github.com/t3brightside/gallerycontent/",
  "license": "GPL-2.0-or-later",
  "authors": [
      {
          "name": "Brightside OÜ – TYPO3 development and hosting specialised web agency",
          "role": "Developer",
          "email": "info@t3brightside.com",
          "homepage": "https://t3brightside.com"
      }
  ],
  "require": {
    "typo3/cms-core": "^11.5",
    "typo3/cms-fluid-styled-content": "^11.5",
    "t3brightside/paginatedprocessors": "*"
  },
  "autoload": {
    "psr-4": {
      "Brightside\\Gallerycontent\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "gallerycontent"
    }
  }
}
